{
  "name": "node-example",
  "version": "1.0.0",
  "description": "smk services node-example",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts",
    "serve": "node dist/server.js",
    "migrate": "sequelize db:migrate",
    "seed-undo": "sequelize db:seed:undo:all",
    "seed": "sequelize db:seed:all",
    "populate": "npm run migrate && npm run seed-undo && npm run seed",
    "test": "jest --forceExit --coverage --verbose",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon dist/server.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.4.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/node-cron": "^2.0.3",
    "aws-sdk": "^2.777.0",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "bluebird": "^3.7.2",
    "circular-json": "^0.5.9",
    "coingecko-api": "^1.0.10",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase": "^7.24.0",
    "firebase-admin": "^9.4.1",
    "helmet": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-cron": "^2.0.3",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "swagger-ui-express": "^4.1.4",
    "twilio": "^3.55.0",
    "uuid": "^8.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/circular-json": "^0.4.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.6",
