    "concurrently": "^5.2.0",
    "eslint": "^7.1.0",
    "husky": "^4.3.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.2.1",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
